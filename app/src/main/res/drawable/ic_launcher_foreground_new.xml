<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Main Hexagonal Crystal Lock -->
    <group android:translateX="54" android:translateY="54">
        
        <!-- Outer Glow -->
        <path
            android:pathData="M0,-30L26,-15L26,15L0,30L-26,15L-26,-15Z"
            android:fillColor="#00D4FF"
            android:fillAlpha="0.2"/>
        
        <!-- Main Hexagon Body -->
        <path
            android:pathData="M0,-24L20.8,-12L20.8,12L0,24L-20.8,12L-20.8,-12Z"
            android:fillColor="#4ECDC4"/>
        
        <!-- Lock Shackle -->
        <path
            android:pathData="M-8,-12Q-8,-20 0,-20Q8,-20 8,-12"
            android:strokeWidth="3"
            android:strokeColor="#FFD93D"
            android:fillColor="@android:color/transparent"
            android:strokeLineCap="round"/>
        
        <!-- Keyhole Circle -->
        <path
            android:pathData="M0,-4m-4,0a4,4 0,1 1,8 0a4,4 0,1 1,-8 0"
            android:fillColor="#1A1A2E"/>
        
        <!-- Keyhole Slot -->
        <path
            android:pathData="M0,0L0,6"
            android:strokeWidth="3"
            android:strokeColor="#1A1A2E"
            android:strokeLineCap="round"/>
        
        <!-- Crystal Facet Lines -->
        <path
            android:pathData="M-20.8,-12L0,-24L20.8,-12"
            android:strokeWidth="1"
            android:strokeColor="#FFFFFF"
            android:strokeAlpha="0.6"/>
            
        <path
            android:pathData="M-20.8,12L0,0L20.8,12"
            android:strokeWidth="1"
            android:strokeColor="#FFFFFF"
            android:strokeAlpha="0.4"/>
            
        <!-- Border -->
        <path
            android:pathData="M0,-24L20.8,-12L20.8,12L0,24L-20.8,12L-20.8,-12Z"
            android:strokeWidth="1.5"
            android:strokeColor="#00D4FF"
            android:fillColor="@android:color/transparent"/>
            
    </group>
    
</vector>